{
  "name": "jquery-flipper",
  "title": "jquery-flipper",
  "description": "A simple way to add a CSS3 flip effect to your content.",
  "version": "2.0.0",
  "homepage": "https://github.com/cameronjroe/jquery-flipper",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "start": "npm run server",
    "webpack": "webpack",
    "build": "webpack -p --config webpack.prod.babel.js",
    "server": "webpack-dev-server --content-base ./demo --hot --inline --no-info"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cameronjroe/jquery-flipper.git"
  },
  "bugs": "https://github.com/cameronjroe/jquery-flipper/issues",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/cameronjroe/jquery-flipper/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "jquery": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.2",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.1",
    "cssnano": "^3.4.0",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "imports-loader": "^0.6.5",
    "node-bourbon": "^4.2.3",
